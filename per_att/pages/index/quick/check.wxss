/* pages/index/quick/check.wxss */
.container{
   width: 98%;
   background-color: #FBFBFC;
   /* border: 1px solid red; */
   margin: 0 auto;
}
.calendar {
   background-color:#FBFBFC;  /*背景色为白色*/
   padding-top: 10px;       /*上内边距为 10px*/
   border-radius: 15px;     /*添加边框圆角*/
}
.header {
   font-size: large;
   color: #000000; 
   /* border: 1rpx solid red; */
   width: 200rpx;
   text-align: left !important; 
}
.header::after{
   content: "";
   width: 40rpx;
   height: 40rpx;
   top: 24rpx;
   position: absolute;
   left: 210rpx;
   background: url("data:image/png;base64,") no-repeat;
   background-size: 100%;
   /* border: 1rpx solid red; */
}
.board {
   color: #000000;
   font-weight: bold;
}
.cal{
   position: relative;
}
.month-check{
   position: absolute;
   right: 20rpx;
   top: 20rpx;
   color: #5F75E2;
   font-size: 28rpx;
}
.check-info{
   margin-top: 30rpx;
   width: 100%;
   /* height: 280rpx; */
   display: flex;
   flex-direction: row;
   justify-content: space-around;
}
.check-info .left-info,.right-info{
   padding-left: 26rpx;
   display: flex;
   flex-direction: column;
   justify-content: start;
   /* position: relative; */
}
.item-info::before,.start-time::before,.end-time::before{
   content: "";
   display: inline-block;
   background-color: #06D9BB;
   border-radius: 50%;
   width: 20rpx;
   height: 20rpx;
   margin-right: 10rpx;
  
}
.item-info{
   margin-bottom: 20rpx;
}
.left-info{
   width: 48%;
   height: 260rpx;
   /* border: 1rpx solid red; */
}
.right-info{
   width: 48%;
   height: 260rpx;
   /* border: 1rpx solid red; */
}
.sign-in{
   width: 100%;
   height: 320rpx;
   /* border: 1rpx solid red; */
}
.sign-title{
   display: inline-block;
   margin: 24rpx 0;
   font-weight: 700;
}
.sign-start,.sign-end{
   width: 100%;
   height: 100rpx;
   /* border: 1rpx solid red; */
   display: flex;
   flex-direction: row;
   font-size: 27rpx;
}
.start-time,.end-time{
   display: inline-block;
   width: 132rpx;
   height: 100rpx;
   line-height: 40rpx;
   /* border: 1rpx solid red; */
}
.s-address{
   margin-bottom: 10rpx;
}
.month-check-dialog{
   width: 100%;
   height: 54%;
   position: fixed;
   bottom: 0rpx;
   left: 0rpx;
   background-color: #FFFFFF;
}
.dialog-item{
   width: 100%;
   box-sizing: border-box;
   height: 100rpx;
   /* border: 1px solid red; */
   display: flex;
   flex-direction: row;
   justify-content: space-between;
   padding: 0 20rpx;
   line-height: 100rpx;
   font-size: 30rpx;
   border-bottom: 1px solid #E7E6E6;
}

